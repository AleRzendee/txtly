generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Contato {
  id        String   @id @default(uuid())
  nome      String
  email     String   @unique
  assunto   String
  createdAt DateTime @default(now())
}

model Usuario {
  id        String   @id @default(uuid())
  nome      String
  email     String   @unique
  senha     String
  createdAt DateTime @default(now())
}
